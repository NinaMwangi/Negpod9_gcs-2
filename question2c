#!/bin/bash

import random
import math

# Generate two random numbers
num1 = random.randint(1, 100000)
num2 = random.randint(1, 100000)

# Compute the square roots of the numbers
sqrt1 = int(math.sqrt(num1))
sqrt2 = int(math.sqrt(num2))

# Print the numbers and their square roots
print(f"The first random number generated is {num1}")
print(f"The square root is {sqrt1}")
print(f"The second random number generated is {num2}")
print(f"The square root is {sqrt2}")

# Compute and print the sum of the square roots
sum_sqrt = sqrt1 + sqrt2
print(f"The sum of their square roots is {sum_sqrt}")
